{
    "entryFile": "src/index.ts",
    "noImplicitAdditionalProperties": "throw-on-extras",
    "routes": {
        "routesDir": "src/routes",
        "authenticationModule": "src/controllers/authMiddleware.ts"
    },
    "spec": {
        "outputDirectory": "public",
        "specVersion": 3,
        "securityDefinitions": {
            "access_token": {
                "type": "http",
                "name": "access_token",
                "scheme": "bearer"
            }
        }
    }
}
